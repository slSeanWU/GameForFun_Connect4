/**
	The Main class initializes a new Connect 4 game and starts it.
*/
 
class Main
{
	/** Initializes a new game and starts it */
	function void main()
	{
        var HomeScreen hs;

        let hs = HomeScreen.new();
        do hs.run();
        do hs.dispose();

        return;
	}
}
